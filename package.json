{
  "name": "copilot-proxy",
  "version": "1.2.1",
  "description": "HTTP proxy for GitHub Copilot API",
  "author": "Hank Chiu (https://hankchiu.tw)",
  "homepage": "https://github.com/hankchiutw/copilot-proxy",
  "license": "MIT",
  "keywords": ["copilot", "proxy", "openai"],
  "type": "module",
  "main": "dist/server/index.mjs",
  "bin": {
    "copilot-proxy": "bin/cli.js"
  },
  "files": ["dist", "READEME.md", "package.json", "bin"],
  "scripts": {
    "dev": "node --env-file=.env ./node_modules/vinxi/bin/cli.mjs dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "check": "biome check --apply src",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@solidjs/meta": "0.29.4",
    "@solidjs/router": "0.15.3",
    "@solidjs/start": "1.1.4",
    "@tinyhttp/app": "^2.2.1",
    "@tinyhttp/logger": "^2.1.0",
    "daisyui": "^5.0.30",
    "dotenv": "^16.5.0",
    "lucide-solid": "^0.503.0",
    "milliparsec": "^2.3.0",
    "node-persist": "^4.0.4",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "sirv": "^3.0.1",
    "solid-js": "^1.9.5",
    "uuid": "^11.1.0",
    "vinxi": "0.5.6",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.0.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.25.0",
    "jsdom": "26.1.0",
    "nodemon": "^3.1.10",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
